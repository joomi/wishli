<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL"
                    readonly="true" class="readonly"
                    description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="listid" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_LISTID" description="COM_WISHLI_FORM_DESC_GIFT_LISTID" 
                
                
                filter="safehtml" /> 

       <field name="title" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_TITLE" description="COM_WISHLI_FORM_DESC_GIFT_TITLE" 
                
                
                filter="safehtml" /> 

       <field name="link" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_LINK" description="COM_WISHLI_FORM_DESC_GIFT_LINK" 
                
                
                filter="safehtml" /> 

       <field name="desc" type="editor" buttons="true"
                class="inputbox"
                filter="safehtml"
                
                
                 label="COM_WISHLI_FORM_LBL_GIFT_DESC" description="COM_WISHLI_FORM_DESC_GIFT_DESC"  /> 

       <field name="status" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_STATUS" description="COM_WISHLI_FORM_DESC_GIFT_STATUS" 
                
                
                filter="safehtml" /> 

       <field name="image" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_IMAGE" description="COM_WISHLI_FORM_DESC_GIFT_IMAGE" 
                
                
                filter="safehtml" /> 

       <field name="budget" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_BUDGET" description="COM_WISHLI_FORM_DESC_GIFT_BUDGET" 
                
                
                filter="safehtml" /> 

       <field name="price" type="text" size="40" class="inputbox"
                 label="COM_WISHLI_FORM_LBL_GIFT_PRICE" description="COM_WISHLI_FORM_DESC_GIFT_PRICE" 
                
                
                filter="safehtml" /> 

         <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1">
                <option value="1">JPUBLISHED</option>
                <option value="0">JUNPUBLISHED</option>
            </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
                <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
